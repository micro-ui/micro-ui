{
  "name": "micro-ui-alert",
  "version": "0.0.5",
  "description": "micro-ui-alert component",
  "author": "Jon Carlson",
  "repository": "https://github.com/joncarlson/micro-ui",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joncarlson/micro-ui/issues"
  },
  "homepage": "https://github.com/joncarlson/micro-ui#readme",
  "main": "dist/index.js",
  "keywords": [
    "inferno",
    "react",
    "micro-ui",
    "performance",
    "web component",
    "micro-ui-alert",
    "alert"
  ],
  "directories": {
    "src": "src"
  },
  "watch": {
    "build:scss": {
      "patterns": [
        "src"
      ],
      "extensions": "scss",
      "quiet": true,
      "legacyWatch": true
    },
    "build:js": {
      "patterns": [
        "src"
      ],
      "extensions": "js",
      "quiet": true,
      "legacyWatch": true
    }
  },
  "scripts": {
    "build:scss": "cp src/*scss dist/",
    "build:js": "babel src -d dist",
    "build": "npm run build:scss && npm run build:js",
    "build:watch": "npm-watch",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.6",
    "babel-loader": "^7.1.2",
    "babel-plugin-inferno": "^3.4.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-undefined-to-void": "^6.8.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "inferno": "^3.10.1",
    "npm-watch": "^0.3.0",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "micro-ui-styles": "^0.0.8"
  }
}

{
  "name": "micro-ui-styles",
  "version": "0.0.9",
  "description": "micro-ui-styles",
  "author": "Jon Carlson",
  "repository": "https://github.com/micro-ui/micro-ui",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/micro-ui/micro-ui/issues"
  },
  "homepage": "https://github.com/micro-ui/micro-ui#readme",
  "main": "dist/index.css",
  "keywords": [
    "inferno",
    "react",
    "micro-ui",
    "performance",
    "web component"
  ],
  "directories": {
    "src": "src"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "scss",
      "quiet": true,
      "legacyWatch": true
    }
  },
  "scripts": {
    "build": "cp -r src/ dist/",
    "build:watch": "npm-watch",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "material-components-web": "^0.28.0"
  },
  "devDependencies": {
    "npm-watch": "^0.3.0"
  }
}
